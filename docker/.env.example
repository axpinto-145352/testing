# Gap Analysis Tool - Environment Configuration
# Copy to .env and fill in values before deployment

# n8n Configuration
N8N_HOST=gap-analysis.yourdomain.com
N8N_ENCRYPTION_KEY=replace-with-random-32-char-string
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=replace-with-strong-password

# PostgreSQL
POSTGRES_USER=n8n
POSTGRES_PASSWORD=replace-with-strong-password

# Data Source Sheet IDs (Google Sheets or equivalent)
MASTER_ROSTER_SHEET_ID=
SURVEY_CONFIG_SHEET_ID=
RESPONSES_SHEET_ID=
DEMOGRAPHICS_SHEET_ID=
ARCHIVE_SHEET_ID=
ANALYTICS_SHEET_ID=

# Email Configuration
NOTIFICATION_FROM_EMAIL=gap-analysis@yourdomain.com
ANALYST_EMAIL=analyst@yourdomain.com

# Dashboard
DASHBOARD_URL=https://gap-analysis.yourdomain.com/dashboard
